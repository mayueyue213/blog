<!DOCTYPE HTML>
<html lang="<%= config.language %>">
<%- partial('_partial/head') %>

<body>
    <% if (!is_home()) { %>
        <%- partial('_partial/header') %>
    <% } %>

    <%- body %>

    <% if (!is_home()) { %>
        <%- partial('_partial/footer') %>
    <% } %>


    <%- partial('_partial/search-modal') %>
    <%- partial('_partial/back-top') %>

    <script src="<%- url_for(config.libs.js.materialize) %>"></script>
    <script src="<%- url_for(config.libs.js.masonry) %>"></script>
    <script src="<%- url_for(config.libs.js.aos) %>"></script>
    <script src="<%- url_for(config.libs.js.scrollProgress) %>"></script>
    <script src="<%- url_for(config.libs.js.lightgallery) %>"></script>
    <script src="<%- url_for(config.libs.js.matery) %>"></script>
    <script src="<%- url_for(config.libs.js.search) %>"></script>
    <script type="text/javascript" src="<%- url_for(config.libs.js.codeBlockFuction) %>"></script>
    <script type="text/javascript" src="<%- url_for(config.libs.js.codeLang) %>"></script>
    <script type="text/javascript" src="<%- url_for(config.libs.js.codeCopy) %>"></script>
    <script type="text/javascript" src="<%- url_for(config.libs.js.codeShrink) %>"></script>
    <%- partial('_partial/baidu-analytics') %>
    <%- partial('_partial/baidu-push') %>
    <% if (config.busuanziStatistics && config.busuanziStatistics.enable) { %>
    <script async src="<%- url_for(config.libs.js.busuanzi) %>"></script>
    <% } %>

    <% if (config.tidio.enable) { %>
        <script src="//code.tidio.co/<%- config.tidio.public_key %>.js"></script>
        <script>
            $(document).ready(function () {
                setInterval(change_Tidio, 50);
                function change_Tidio() {
                    var tidio=$("#tidio-chat iframe");
                    if(tidio.css("display")=="block"&& $(window).width()>977 ){
                        document.getElementById("tidio-chat-iframe").style.bottom= ($("div#backTop.top-scroll").css("display")=="none" &&$(window).width()>977)>0? "-40px" : ($("div.toc-title").length&&$(window).width()>977)>0?"<%if(config.toc.showToggleBtn){%>85px<%}else{%>20px<%}%>":"20px";
                        document.getElementById("tidio-chat-iframe").style.right="-15px";
                        document.getElementById("tidio-chat-iframe").style.height=parseInt(tidio.css("height"))>=520?"520px":tidio.css("height");
                        document.getElementById("tidio-chat-iframe").style.zIndex="997";
                    }
                    else if(tidio.css("display")=="block"&&$(window).width()>601 &&$(window).width()<992 ){
                        document.getElementById("tidio-chat-iframe").style.bottom= ($("div#backTop.top-scroll").css("display")=="none" && 601< $(window).width()<992)>0? "-40px":"20px" ;
                        document.getElementById("tidio-chat-iframe").style.right="-15px";
                        document.getElementById("tidio-chat-iframe").style.zIndex="997";
                    }
                    else if(tidio.css("display")=="block"&&$(window).width()<601 && parseInt(tidio.css("height"))<230){
                        document.getElementById("tidio-chat-iframe").style.bottom= ($("div#backTop.top-scroll").css("display")=="none" && $(window).width()<601)>0? "-10px":"45px" ;
                        document.getElementById("tidio-chat-iframe").style.zIndex="997";
                    }
                    if( tidio.css("display")=="block"&&$(window).width()<601 && parseInt(tidio.css("height"))>=230){
                        document.getElementById("tidio-chat-iframe").style.zIndex="998";
                    }
                }
            });
        </script>
    <% } %>

    <% if (config.daovoice.enable) { %>
    <script>
        (function (i, s, o, g, r, a, m) {
            i["DaoVoiceObject"] = r;
            i[r] = i[r] || function () {
                (i[r].q = i[r].q || []).push(arguments)
            }, i[r].l = 1 * new Date();
            a = s.createElement(o), m = s.getElementsByTagName(o)[0];
            a.async = 1;
            a.src = g;
            a.charset = "utf-8";
            m.parentNode.insertBefore(a, m)
        })(window, document, "script", ('https:' == document.location.protocol ? 'https:' : 'http:') +
            "//widget.daovoice.io/widget/6984b559.js", "daovoice")
        daovoice('init', {
            app_id: "<%- config.daovoice.app_id %>"
        });
        daovoice('update');
    </script>
    <% } %>

	<% if (!is_home() && config.tuxiaochao.enable) { %>
	    <div style="position:fixed;bottom:125px;right:9px;cursor: pointer;z-index:10">
	        <a title="兔小巢" href="https://support.qq.com/products/<%- config.tuxiaochao.productId %>"><img src="<%- config.tuxiaochao.icon %>"></a>
	    </div>
	<%}%>
    <% if (config.canvas_nest.enable) { %>
    <script type="text/javascript" color="<%- config.canvas_nest.color %>"
        pointColor="<%- config.canvas_nest.pointColor %>" opacity='<%- config.canvas_nest.opacity %>'
        zIndex="<%- config.canvas_nest.zIndex %>" count="<%- config.canvas_nest.count %>"
        src="<%- url_for(config.libs.js.canvas_nest) %>"></script>
    <% } %>

    <% if (config.ribbon.enable) { %>
    <% var ribbonSrc = config.ribbon.clickChange ? config.libs.js.ribbon : config.libs.js.ribbonRefresh; %>
    <script type="text/javascript" size="<%- config.ribbon.size %>" alpha='<%- config.ribbon.alpha %>'
        zIndex="<%- config.ribbon.zIndex %>" src="<%- url_for(ribbonSrc) %>" async="async"></script>
    <% } %>

    <% if (config.ribbon_dynamic.enable) { %>
    <script type="text/javascript" src="<%- url_for(config.libs.js.ribbon_dynamic) %>" async="async"></script>
    <% } %>

    <% if (config.instantpage.enable) { %>
    <script src="<%- url_for(config.libs.js.instantpage) %>" type="module"></script>
    <% } %>

</body>
</html>
