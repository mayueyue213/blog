<header class="navbar-fixed">
    <nav id="headNav" class="bg-color nav-transparent">
        <div id="navContainer" class="nav-wrapper container">
            <div class="brand-logo">
                <a href="<%- url_for() %>" class="waves-effect waves-light">
                    <% if (config.logo !== undefined && config.logo.length > 0) { %>
                    <img src="<%- config.jsDelivr.url %><%- url_for(config.logo) %>" class="logo-img" alt="LOGO">
                    <% } %>
                    <% if (config.navTitle !== undefined) { %>
                        <span class="logo-span"><%= config.navTitle %></span>
                    <% } %>                    
                </a>
            </div>
            <%- partial('_partial/navigation') %>
        </div>
    </nav>
</header>
